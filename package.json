    "sharedb": "^1.3.4",
    "slate": "^0.58.1"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.6",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "jest": "^26.0.1",
    "lodash": "^4.17.15",
    "ot-fuzzer": "^1.2.1",
    "ts-jest": "^26.1.0",
